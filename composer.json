{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "aws/aws-sdk-php": "^3.98",
        "consoletvs/charts": "^6.5",
        "doctrine/annotations": "^1.6",
        "doctrine/couchdb": "^1.0@beta",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "filp/whoops": "^2.3",
        "fzaninotto/faker": "^1.8",
        "graylog2/gelf-php": "^1.6",
        "guzzlehttp/guzzle": "^6.3",
        "hoa/console": "^3.17",
        "ircmaxell/random-lib": "^1.2",
        "khill/datatableplus": "dev-master",
        "khill/lavacharts": "3.1.*",
        "laravel/framework": "5.8.*",
        "laravel/telescope": "^2.0",
        "laravel/tinker": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-azure": "^1.0",
        "league/flysystem-cached-adapter": "^1.0",
        "league/flysystem-eventable-filesystem": "^1.0",
        "league/flysystem-rackspace": "^1.0",
        "league/flysystem-sftp": "^1.0",
        "league/flysystem-webdav": "^1.0",
        "league/flysystem-ziparchive": "^1.0",
        "nexmo/client": "^1.8",
        "pda/pheanstalk": "^4.0",
        "php-amqplib/php-amqplib": "^2.9",
        "php-console/php-console": "^3.1",
        "predis/predis": "^1.1",
        "psr/event-dispatcher": "^1.0",
        "pusher/pusher-php-server": "^3.4",
        "ramsey/uuid-console": "^1.1",
        "ramsey/uuid-doctrine": "^1.5",
        "rollbar/rollbar": "^1.8",
        "ruflin/elastica": "^6.1",
        "sentry/sentry": "^2.1",
        "spatie/flysystem-dropbox": "^1.1",
        "srmklive/flysystem-dropbox-v2": "^1.0",
        "symfony/browser-kit": "^4.3",
        "symfony/config": "^4.3"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
